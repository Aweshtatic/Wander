<div >
    <% if (success && success.length) { %>
        <div class="alert alert-primary mt-2 col-5 offset-3" role="alert">
        <%= success %>
        </div>
    <% } %>

    <% if (error && error.length) { %>
        <div class="alert alert-danger mt-2 col-5 offset-3" role="alert">
        <%= error %>
        </div>
    <% } %>
    
    
</div>